const x = 7;
x = 10; // This will throw an error because you cannot reassign a const variable

var x = 7;
function example() {
  var x = 10;
  console.log(x); // Output: 10
}
console.log(x); // Output: 7

let x = 7;
if (true) {
  let x = 10;
  console.log(x); // Output: 10
}
console.log(x); // Output: 7






























let x = 2;
let y = 3;
let code = "x + y";
let result = eval(code); // Evaluates the string as JavaScript code
console.log(result); // Outputs 5